{
  "name": "npm-prologue",
  "version": "1.0.0",
  "description": "NPM package/project bootstrapper for Junyoung Clare Jang",
  "keywords": [
    "boilerplate",
    "bootstraper",
    "package",
    "project",
    "npm"
  ],
  "homepage": "https://github.com/Ailrun/npm-prologue#readme",
  "bugs": "https://github.com/Ailrun/npm-prologue/issues",
  "license": "MIT",
  "author": {
    "name": "Junyoung Clare Jang",
    "email": "jjc9310@gmail.com",
    "url": "https://Ailrun.github.io"
  },
  "files": [
    "./dist"
  ],
  "main": "./dist/index.js",
  "bin": {
    "npm-prologue": "./dist/npm-prologue.js"
  },
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ailrun/npm-prologue.git"
  },
  "scripts": {
    "build": "npm-run-all -s clean build:compile build:permission",
    "build:compile": "tsc -p ./tsconfig.json",
    "build:permission": "node ./scripts/build/permission.js",
    "clean": "rimraf ./dist",
    "watch": "tsc -w --preserveWatchOutput -p ./tsconfig.json",
    "test": "npm-run-all -s test:*",
    "test:behaviour": "jest --config=./config/jest/jest.config.js",
    "test:styles": "npm-run-all -s test:styles:*",
    "test:styles:src": "tslint -c ./config/tslint/tslint.base.json -p ./tsconfig.json",
    "test:styles:test": "tslint -c ./config/tslint/tslint.test.json -p ./test/behaviour/tsconfig.json",
    "test:types": "dtslint ./test/types"
  },
  "dependencies": {
    "commander": "^2.17.0",
    "figlet": "^1.2.0",
    "gradient-string": "^1.1.0",
    "hosted-git-info": "^2.7.1",
    "inquirer": "^6.0.0",
    "mkdirp": "^0.5.1",
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/commander": "^2.12.2",
    "@types/cross-spawn": "^6.0.0",
    "@types/figlet": "^1.2.0",
    "@types/gradient-string": "^1.1.0",
    "@types/inquirer": "0.0.42",
    "@types/jest": "^23.3.0",
    "@types/memory-fs": "^0.3.1",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^10.5.3",
    "@types/ramda": "^0.25.36",
    "@types/rimraf": "^2.0.2",
    "commitlint": "^7.0.0",
    "cross-spawn": "^6.0.5",
    "dtslint": "^0.3.0",
    "husky": "^1.0.0-rc.13",
    "jest": "^23.4.1",
    "memory-fs": "^0.4.1",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "ts-jest": "^22.4.6",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2"
  }
}
