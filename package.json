{
  "name": "npm-prologue",
  "version": "1.0.0",
  "description": "NPM package/project bootstrapper for Junyoung Clare Jang",
  "keywords": [
    "boilerplate",
    "bootstraper",
    "package",
    "project",
    "npm"
  ],
  "homepage": "https://github.com/Ailrun/npm-prologue#readme",
  "bugs": "https://github.com/Ailrun/npm-prologue/issues",
  "license": "MIT",
  "author": {
    "name": "Junyoung Clare Jang",
    "email": "jjc9310@gmail.com",
    "url": "https://Ailrun.github.io"
  },
  "files": [
    "*"
  ],
  "main": "./dist/index.js",
  "bin": {
    "npm-prologue": "./dist/npm-prologue.js"
  },
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ailrun/npm-prologue.git"
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.json",
    "watch": "tsc -w --preserveWatchOutput -p ./tsconfig.json",
    "test": "npm-run-all -s test:*",
    "test:behaviour": "cross-env TS_NODE_PROJECT=./test/behaviour/tsconfig.json mocha -r ts-node/register ./test/behaviour/**/*.ts",
    "test:types": "dtslint ./test/types"
  },
  "dependencies": {
    "figlet": "^1.2.0",
    "gradient-string": "^1.1.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.5.3",
    "chai": "^4.1.2",
    "commitlint": "^7.0.0",
    "cross-env": "^5.2.0",
    "dtslint": "^0.3.0",
    "husky": "^1.0.0-rc.13",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "ts-node": "^7.0.0",
    "typescript": "^2.9.2"
  }
}
